@echo off
REM ============================================================================
REM Конфигурационный файл для указания пути к Java 21
REM ============================================================================
REM Скопируйте этот файл в java21-config.bat и укажите путь к Java 21
REM
REM Примеры путей:
REM   set "JAVA21_HOME=C:\Program Files\Java\jdk-21"
REM   set "JAVA21_HOME=D:\java\jdk-21.0.1"
REM   set "JAVA21_HOME=C:\Program Files\Eclipse Adoptium\jdk-21.0.1-hotspot"
REM ============================================================================

REM Укажите здесь путь к установленной Java 21
set "JAVA21_HOME=C:\Program Files\Java\jdk-21"

REM Путь к исполняемому файлу java.exe
set "JAVA21_EXE=%JAVA21_HOME%\bin\java.exe"
